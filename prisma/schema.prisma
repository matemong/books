generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
    id String @id @map("_id") @default(auto()) @db.ObjectId
    email String @unique
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
  }